{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PASSWORD": "@PASSWORD",
    "PORT": "@PORT"
  },
  "build": {
    "env": {
      "PASSWORD": "@PASSWORD",
      "PORT": "@PORT"
    }
  }
}